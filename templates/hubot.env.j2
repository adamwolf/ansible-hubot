#
# File: hubot.env - Hubot variables for HipChat and more
#

{% for key, value in hubot_env.iteritems() -%}
{{ key }}={{ value }}
{% endfor %}

{% if hubot_slack_token is defined -%}
HUBOT_SLACK_TOKEN={{hubot_slack_token}}
export HUBOT_SLACK_TOKEN
{% endif %}

export {% for key, value in hubot_env.iteritems() -%}{{ key }}{{- ' ' if not loop.last else '' }}{% endfor %}

